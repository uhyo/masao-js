{
  "name": "masao",
  "version": "0.1.3",
  "description": "Utilities for Canvas Masao.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:tsc": "tsc",
    "build:json": "cpx data/*.json dist/ -u",
    "build:json5": "./build/json5.js data/ dist/",
    "watch": "npm-run-all --parallel watch:*",
    "watch:tsc": "tsc --watch",
    "watch:json": "cpx data/*.json dist/ -uw",
    "watch:json5": "./build/json5.js data/ dist/ --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uhyo/masao-js"
  },
  "keywords": [
    "masao"
  ],
  "author": "uhyo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uhyo/masao-js/issues"
  },
  "homepage": "https://github.com/uhyo/masao-js",
  "dependencies": {
    "extend": "^3.0.0",
    "jsonschema": "^1.1.1"
  },
  "devDependencies": {
    "@types/extend": "^2.0.30",
    "@types/node": "^6.0.52",
    "cpx": "^1.5.0",
    "glob": "^7.1.1",
    "json5": "^0.5.1",
    "npm-run-all": "^4.0.1",
    "typescript": "^2.1.4",
    "watch": "^1.0.1"
  }
}
